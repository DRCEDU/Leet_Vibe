{
  "mcpServers": {
    "document-image-extractor": {
      "command": "uv",
      "args": [
        "--directory",
        "./document-image-extractor-mcp",
        "run",
        "document-image-extractor-mcp"
      ],
      "disabled": false,
      "autoApprove": [
        "list_supported_formats",
        "validate_document",
        "get_document_info"
      ]
    },
    "mdconverter-mcp": {
      "command": "./mdconverter-mcp/venv/bin/python",
      "args": [
        "-m",
        "mdconverter_mcp"
      ],
      "cwd": "./mdconverter-mcp",
      "disabled": false,
      "autoApprove": [
        "list_supported_formats",
        "validate_html_file",
        "get_html_metadata"
      ]
    }
  }
}